<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sunshine.sihuo" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RequestFriends"
            android:label="@string/title_activity_request_friends" >
        </activity>
        <activity
            android:name=".ShowKindActivity"
            android:label="@string/title_activity_show_kind" >
        </activity>
        <activity
            android:name=".ShowMoreActivity"
            android:label="@string/title_activity_show_more" >
        </activity>
    </application>

</manifest>
